{
  "expo": {
    "name": "nonoArt",
    "slug": "nonoArt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "sdkVersion": "40.0.0",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2d2d2d"
