    "android": {
      "package": "com.antogor.nonoArt",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#2d2d2d"
      }
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    }
  }
